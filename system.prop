persist.vendor.ssr.enable_ramdumps=0
vendor.bluetooth.startbtlogger=false
vendor.swvdec.log.level=0

# wifi scanning interval(higher value=powersave)
wifi.supplicant_scan_interval=600

# Xiaomi Speed Install Feature 
ro.miui.pm.install.speedinstall=/data/apk-tmp

#Enable Xiaomi13/14 LTPO back
persist.vendor.disable_idle_fps.threshold=492

video.accelerate.hw=1